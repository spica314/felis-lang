#inductive eq : (A : Type) -> (x : A) -> A -> Prop {
    eq_refl: (A : Type) -> (x : A) -> eq A x x,
}

#inductive nat : Set {
    O: nat,
    S: nat -> nat,
}

#definition add : (n : nat) -> (m : nat) -> nat {
    #match n {
        O => m,
        S p => S (add p m),
    }
}

#theorem add_wrong_calculation : eq nat (add (S O) (S O)) (add O (S (S O))) {
    eq_refl nat (S (S O))
}